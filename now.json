{
	"version": 2,
	"routes":
	[
		{"src": "/(?<path>.*?)/+", "status": 307, "headers": {"location": "/$path"}, "methods": ["GET", "HEAD"]},
		{"src": "/", "dest": "/api/list.js"},
		{"src": "/(?<name>.*)/feedback", "dest": "/api/feedback.js?name=$1", "methods": ["POST"]},
		{"src": "/fonts/(?<name>.*)", "methods": ["GET", "HEAD"]},
		{"src": "/style.css", "methods": ["GET", "HEAD"]},
		{"src": "/license", "status": 307, "headers": {"location": "https://github.com/Zambonifofex/stories#license"}, "methods": ["GET", "HEAD"]},
		{"src": "/(?<name>.*)", "dest": "/api/story.js?name=$1", "methods": ["GET", "HEAD"]}
	],
	"env":
	{
		"google_credentials_json": "@google-credentials",
		"bucket_name": "@bucket-name"
	},
	"github": {"silent": true}
}
