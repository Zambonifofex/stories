{
	"version": 2,
	"routes":
	[
		{"src": "/(?<path>.*?)/+", "status": 307, "headers": {"location": "/$path"}, "methods": ["GET", "HEAD"]},
		{"src": "/", "dest": "/api/list.js"},
		{"src": "/(?<name>.*)/feedback", "dest": "/api/feedback.js?name=$name", "methods": ["POST"]},
		{"src": "/fonts/(.*)", "methods": ["GET", "HEAD"]},
		{"src": "/style.css", "methods": ["GET", "HEAD"]},
		{"src": "/script.js", "methods": ["GET", "HEAD"]},
		{"src": "/licenses.txt", "methods": ["GET", "HEAD"]},
		{"src": "/license", "status": 307, "headers": {"location": "https://github.com/Zambonifofex/stories#license"}, "methods": ["GET", "HEAD"]},
		{"src": "/(?<name>.*)", "dest": "/api/story.js?name=$name", "methods": ["GET", "HEAD"]}
	],
	"env":
	{
		"mongo_database": "@mongo-database",
		"mongo_url": "@mongo-url"
	},
	"github": {"silent": true}
}
